{
  "name": "@_nu/react-dialog",
  "version": "1.0.6",
  "description": "A fully accessible、flexible、unstyled React dialog component",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "es/index.js",
  "author": "ziven27",
  "license": "MIT",
  "repository": "git@github.com:nu-system/react-dialog.git",
  "bugs": "https://github.com/nu-system/react-dialog/issues",
  "homepage": "https://nu-system.github.io/react/dialog/",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "focus-trap-react": "^6.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "react-component",
    "ui",
    "no-ui",
    "nu-system",
    "react",
    "react-ui",
    "dialog",
    "modal"
  ]
}
